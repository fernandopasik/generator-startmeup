{
  "name": "generator-startmeup",
  "version": "0.5.0",
  "description": "StartMeUp Yeoman Generator for starting a project with dotfiles and some common tools",
  "keywords": [],
  "homepage": "https://github.com/fernandopasik/generator-startmeup",
  "bugs": "https://github.com/fernandopasik/generator-startmeup/issues",
  "license": "MIT",
  "author": "Fernando Pasik <fernando@pasik.com.ar> (http://fernandopasik.com/)",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/fernandopasik/generator-startmeup.git"
  },
  "main": "generators/app/index.js",
  "scripts": {
    "start": "npm run watch",
    "clean": "rm -rf .tmp coverage",
    "lint": "eslint .",
    "test": "mocha",
    "pretest": "npm run clean",
    "cov": "istanbul cover _mocha --report lcovonly --report html",
    "precov": "npm run clean",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "precoveralls": "npm run -s cov",
    "travis": "npm run -s lint && npm run -s coveralls",
    "watch": "watch 'npm run -s lint; npm run -s cov' generators/ test/"
  },
  "files": [
    "generators"
  ],
  "dependencies": {
    "slug": "^0.9.1",
    "yeoman-generator": "^0.22.6",
    "yosay": "^1.1.1"
  },
  "devDependencies": {
    "coveralls": "^2.11.9",
    "eslint": "^2.8.0",
    "eslint-config-fernandopasik": "^0.0.3",
    "istanbul": "^0.4.3",
    "mocha": "^2.4.5",
    "mocha-clean": "^1.0.0",
    "sinon": "^1.17.3",
    "watch": "^0.17.1",
    "yeoman-assert": "^2.2.1",
    "yeoman-test": "^1.1.0"
  },
  "engines": {
    "node": ">=4.0.0",
    "npm": ">=2.14.2"
  }
}
