  "contributors": [],
  "main": "generators/app/index.js",
  "scripts": {
    "prebuild": "del generators",
    "build": "tsc",
    "postbuild": "cpy **/templates/* ../generators/ --cwd=src --parents",
    "check-types": "tsc --noEmit -p tsconfig.all.json",
    "lint": "eslint . --ext ts,js",
    "start": "yarn build -w",
    "test": "jest",
    "verify": "yarn lint && yarn test && yarn build",
    "preversion": "yarn verify"
  },
  "resolutions": {
