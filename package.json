{
  "name": "generator-startmeup",
  "version": "0.34.0",
  "description": "StartMeUp Yeoman Generator for starting a project with dotfiles and some common tools",
  "repository": "fernandopasik/generator-startmeup",
  "homepage": "https://github.com/fernandopasik/generator-startmeup",
  "bugs": "https://github.com/fernandopasik/generator-startmeup/issues",
  "author": "Fernando Pasik <fernando@pasik.com.ar> (https://fernandopasik.com)",
  "contributors": [],
  "license": "MIT",
  "engines": {
    "node": ">=12"
  },
  "main": "generators/app/index.js",
  "typings": "generators/app/index.d.ts",
  "files": [
    "/generators"
  ],
  "keywords": [],
  "scripts": {
    "build": "tsc -p tsconfig.build.json && yarn copy:templates",
    "changelog": "yarn changelog:config && yarn changelog:write && yarn format",
    "changelog:config": "replace-in-files --string='hidden: true' --replacement='hidden: false' node_modules/conventional-changelog-conventionalcommits/writer-opts.js",
    "changelog:write": "conventional-changelog -p conventionalcommits -i CHANGELOG.md -s -r 0",
    "check-types": "tsc --noEmit",
    "copy:templates": "cpy '**/templates/**' ../generators/ --cwd=src --parents",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint . --ext js,ts",
    "postpublish": "pinst --enable",
    "prebuild": "del generators",
    "prepare": "husky install",
    "prepublishOnly": "pinst --disable",
    "preversion": "yarn verify",
    "start": "yarn build -w",
    "test": "jest",
    "test:ci": "jest --coverage --runInBand --ci --reporters=default --reporters=jest-junit",
    "test:coverage": "jest --coverage",
    "verify": "yarn format:check && yarn lint && yarn check-types && yarn test:coverage && yarn build"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "git-remote-origin-url": "^3.1.0",
    "github-username": "^7.0.0",
    "globby": "^11.0.4",
    "inquirer": "^8.2.0",
    "lodash": "^4.17.21",
    "parse-github-url": "^1.0.2",
    "prettier": "^2.5.1",
    "prettier-plugin-organize-imports": "^2.3.4",
    "prettier-plugin-pkg": "^0.11.1",
    "prettier-plugin-sh": "^0.8.1",
    "shelljs": "^0.8.4",
    "sort-package-json": "^1.53.1",
    "yeoman-generator": "^5.4.2",
    "yosay": "^2.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@jest/types": "^27.4.2",
    "@types/eslint": "^8.2.1",
    "@types/inquirer": "^8.1.3",
    "@types/jest": "^27.0.3",
    "@types/lodash": "^4.14.178",
    "@types/mvdan-sh": "^0.5.1",
    "@types/parse-github-url": "^1.0.0",
    "@types/prettier": "^2.4.2",
    "@types/shelljs": "^0.8.10",
    "@types/yeoman-assert": "^3.1.1",
    "@types/yeoman-generator": "^5.2.7",
    "@types/yeoman-test": "^4.0.3",
    "@types/yosay": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "conventional-changelog-cli": "^2.1.1",
    "conventional-changelog-conventionalcommits": "^4.6.1",
    "cpy-cli": "^3.1.1",
    "del-cli": "^4.0.1",
    "eslint": "^8.5.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-security": "^1.4.0",
    "husky": "^7.0.4",
    "jest": "^27.4.5",
    "jest-junit": "^13.0.0",
    "lint-staged": "^12.1.3",
    "pinst": "^2.1.6",
    "replace-in-files-cli": "^2.0.0",
    "ts-jest": "^27.1.2",
    "type-fest": "^2.8.0",
    "typescript": "^4.5.4",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^6.2.0"
  },
  "resolutions": {
    "fast-glob": "^3.2.7"
  },
  "sideEffects": false
}
