{
  "name": "generator-startmeup",
  "version": "0.30.0",
  "description": "StartMeUp Yeoman Generator for starting a project with dotfiles and some common tools",
  "keywords": [],
  "homepage": "https://github.com/fernandopasik/generator-startmeup",
  "bugs": "https://github.com/fernandopasik/generator-startmeup/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/fernandopasik/generator-startmeup.git"
  },
  "license": "MIT",
  "author": "Fernando Pasik <fernando@pasik.com.ar> (https://fernandopasik.com)",
  "contributors": [],
  "main": "generators/app/index.js",
  "bin": {
    "startmeup": "generators/startmeup.js"
  },
  "files": [
    "/generators"
  ],
  "scripts": {
    "prebuild": "del generators",
    "build": "tsc",
    "postbuild": "cpy **/templates/** ../generators/ --cwd=src --parents && chmod +x generators/startmeup.js",
    "prechangelog": "replace-in-files --string='hidden: true' --replacement='hidden: false' node_modules/conventional-changelog-conventionalcommits/writer-opts.js",
    "changelog": "conventional-changelog -p conventionalcommits -i CHANGELOG.md -s",
    "postchangelog": "prettier --write CHANGELOG.md",
    "check-types": "tsc --noEmit -p tsconfig.all.json",
    "format": "prettier --no-error-on-unmatched-pattern --write ** ./* ./.??*",
    "postinstall": "husky install",
    "lint": "eslint . --ext js,ts",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "start": "yarn build -w",
    "test": "jest",
    "verify": "yarn lint && yarn check-types && yarn test && yarn build",
    "preversion": "yarn verify"
  },
  "dependencies": {
    "cross-spawn": "^7.0.3",
    "github-username": "^6.0.0",
    "inquirer": "^8.0.0",
    "lodash": "^4.17.21",
    "prettier": "^2.3.0",
    "prettier-plugin-organize-imports": "^2.1.0",
    "prettier-plugin-packagejson": "^2.2.11",
    "sort-package-json": "^1.50.0",
    "yeoman-generator": "^5.2.0",
    "yosay": "^2.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@jest/types": "^26.6.2",
    "@types/cross-spawn": "^6.0.2",
    "@types/eslint": "^7.2.10",
    "@types/inquirer": "^7.3.1",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.169",
    "@types/prettier": "^2.2.3",
    "@types/yeoman-assert": "^3.1.1",
    "@types/yeoman-generator": "^5.0.0",
    "@types/yeoman-test": "^4.0.1",
    "@types/yosay": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "codecov": "^3.8.2",
    "conventional-changelog-cli": "^2.1.1",
    "conventional-changelog-conventionalcommits": "^4.6.0",
    "cpy-cli": "^3.1.1",
    "del-cli": "^3.0.1",
    "eslint": "7.26.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "lint-staged": "^11.0.0",
    "pinst": "^2.1.6",
    "prettier": "^2.3.0",
    "prettier-plugin-organize-imports": "^2.1.0",
    "prettier-plugin-packagejson": "^2.2.11",
    "replace-in-files-cli": "^1.0.0",
    "ts-jest": "^26.5.6",
    "type-fest": "^1.1.1",
    "typescript": "^4.2.4",
    "watch": "^1.0.2",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^6.0.0"
  },
  "engines": {
    "node": ">=12"
  }
}
