{
  "name": "generator-startmeup",
  "version": "0.5.11",
  "description": "StartMeUp Yeoman Generator for starting a project with dotfiles and some common tools",
  "keywords": [],
  "homepage": "https://github.com/fernandopasik/generator-startmeup",
  "bugs": "https://github.com/fernandopasik/generator-startmeup/issues",
  "license": "MIT",
  "author": "Fernando Pasik <fernando@pasik.com.ar> (http://fernandopasik.com)",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/fernandopasik/generator-startmeup.git"
  },
  "main": "generators/app/index.js",
  "scripts": {
    "postinstall": "cp .npmignore .gitignore",
    "start": "npm run watch",
    "lint": "eslint .",
    "test": "mocha",
    "coverage": "rm -rf coverage .nyc_output && nyc mocha",
    "build": "npm run -s lint && npm run -s coverage",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "travis": "npm run -s build && npm run -s coveralls",
    "watch": "watch 'npm run -s build' generators/ test/"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ],
    "check-coverage": true,
    "lines": 100,
    "functions": 100,
    "branches": 100,
    "statements": 100
  },
  "dependencies": {
    "yeoman-generator": "^0.24.1",
    "yosay": "^1.2.1"
  },
  "devDependencies": {
    "coveralls": "^2.11.15",
    "eslint": "^3.11.1",
    "eslint-config-fernandopasik": "^0.5.1",
    "mocha": "^3.2.0",
    "mocha-clean": "^1.0.0",
    "nyc": "^10.0.0",
    "sinon": "^1.17.6",
    "watch": "^1.0.1",
    "yeoman-assert": "^2.2.1",
    "yeoman-test": "^1.5.1"
  },
  "engines": {
    "node": ">=4.0.0",
    "npm": ">=2.14.2"
  }
}
