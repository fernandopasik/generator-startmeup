{
  "name": "generator-startmeup",
  "version": "0.1.1",
  "description": "StartMeUp Yeoman Generator",
  "private": true,
  "homepage": "https://github.com/fernandopasik/generator-startmeup",
  "author": {
    "name": "Fernando Pasik",
    "email": "fernando@pasik.com.ar"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/fernandopasik/generator-startmeup.git"
  },
  "main": "generators/app/index.js",
  "scripts": {
    "clean": "rm -rf .tmp coverage",
    "jshint": "jshint . --reporter=node_modules/jshint-stylish",
    "jscs": "jscs .",
    "lint": "npm run -s jshint & npm run -s jscs",
    "test": "mocha",
    "pretest": "npm run clean",
    "cov": "istanbul cover _mocha --report lcovonly -- -R spec",
    "precov": "npm run clean",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "precoveralls": "npm run -s cov",
    "dev": "npm run -s lint && npm run cov",
    "travis": "npm run -s lint && npm run coveralls",
    "watch": "watch 'npm run -s dev' generators/ test/"
  },
  "config": {
    "blanket": {
      "pattern": "generators/"
    }
  },
  "files": [
    "generators"
  ],
  "keywords": [],
  "dependencies": {
    "chalk": "^1.1.0",
    "underscore.string": "^3.1.1",
    "yeoman-generator": "^0.20.2",
    "yosay": "^1.0.5"
  },
  "devDependencies": {
    "chai": "^3.2.0",
    "coveralls": "^2.11.3",
    "grunt": "^0.4.5",
    "grunt-cli": "^0.1.13",
    "grunt-contrib-clean": "^0.6.0",
    "grunt-contrib-jshint": "^0.11.2",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-jscs": "^2.0.0",
    "grunt-mocha-istanbul": "^2.4.0",
    "istanbul": "^0.3.17",
    "jscs": "^2.0.0",
    "jshint": "^2.8.0",
    "jshint-stylish": "^2.0.1",
    "load-grunt-tasks": "^3.2.0",
    "mocha": "^2.2.5",
    "mocha-clean": "^0.4.0",
    "sinon": "^1.15.4",
    "watch": "^0.16.0"
  },
  "engines": {
    "node": ">=0.10.0",
    "npm": ">=1.2.10"
  },
  "license": "MIT"
}
