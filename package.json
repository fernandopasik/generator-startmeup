{
  "name": "generator-startmeup",
  "version": "0.5.4",
  "description": "StartMeUp Yeoman Generator for starting a project with dotfiles and some common tools",
  "keywords": [],
  "homepage": "https://github.com/fernandopasik/generator-startmeup",
  "bugs": "https://github.com/fernandopasik/generator-startmeup/issues",
  "license": "MIT",
  "author": "Fernando Pasik <fernando@pasik.com.ar> (http://fernandopasik.com/)",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/fernandopasik/generator-startmeup.git"
  },
  "main": "generators/app/index.js",
  "scripts": {
    "start": "npm run watch",
    "build": "npm run -s lint; npm test",
    "clean": "rm -rf .tmp coverage",
    "lint": "eslint .",
    "test": "nyc mocha",
    "pretest": "npm run clean",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "travis": "npm run -s build && npm run -s coveralls",
    "watch": "watch 'npm run -s build' generators/ test/"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ],
    "check-coverage": true,
    "lines": 100,
    "functions": 100,
    "branches": 100,
    "statements": 100
  },
  "dependencies": {
    "slug": "^0.9.1",
    "yeoman-generator": "^0.23.3",
    "yosay": "^1.1.1"
  },
  "devDependencies": {
    "coveralls": "^2.11.9",
    "eslint": "^2.9.0",
    "eslint-config-fernandopasik": "^0.1.7",
    "mocha": "^2.4.5",
    "mocha-clean": "^1.0.0",
    "nyc": "^6.4.4",
    "sinon": "^1.17.4",
    "watch": "^0.18.0",
    "yeoman-assert": "^2.2.1",
    "yeoman-test": "^1.4.0"
  },
  "engines": {
    "node": ">=4.0.0",
    "npm": ">=2.14.2"
  }
}
