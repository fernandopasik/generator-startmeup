{
  "name": "generator-startmeup",
  "version": "0.28.1",
  "description": "StartMeUp Yeoman Generator for starting a project with dotfiles and some common tools",
  "keywords": [],
  "homepage": "https://github.com/fernandopasik/generator-startmeup",
  "bugs": "https://github.com/fernandopasik/generator-startmeup/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/fernandopasik/generator-startmeup.git"
  },
  "license": "MIT",
  "author": "Fernando Pasik <fernando@pasik.com.ar> (https://fernandopasik.com)",
  "contributors": [],
  "main": "generators/app/index.js",
  "bin": {
    "startmeup": "generators/startmeup.js"
  },
  "files": [
    "/generators"
  ],
  "scripts": {
    "prebuild": "del generators",
    "build": "tsc",
    "postbuild": "cpy **/templates/** ../generators/ --cwd=src --parents && chmod +x generators/startmeup.js",
    "prechangelog": "replace-in-files --string='hidden: true' --replacement='hidden: false' node_modules/conventional-changelog-conventionalcommits/writer-opts.js",
    "changelog": "conventional-changelog -p conventionalcommits -i CHANGELOG.md -s",
    "postchangelog": "prettier --write CHANGELOG.md",
    "check-types": "tsc --noEmit -p tsconfig.all.json",
    "lint": "eslint . --ext ts,js",
    "prettier": "prettier --write ** ./* ./.??*",
    "start": "yarn build -w",
    "test": "jest",
    "verify": "yarn lint && yarn check-types && yarn test && yarn build",
    "preversion": "yarn verify"
  },
  "resolutions": {
    "conventional-changelog-angular": "^5.0.11",
    "github-username": "^5.0.1"
  },
  "dependencies": {
    "@jest/types": "^26.2.0",
    "core-js": "^3.6.5",
    "cross-spawn": "^7.0.3",
    "github-username": "^5.0.1",
    "inquirer": "^7.3.3",
    "lodash": "^4.17.19",
    "prettier": "^2.0.5",
    "prettier-plugin-organize-imports": "^1.1.1",
    "prettier-plugin-packagejson": "^2.2.5",
    "sort-package-json": "^1.44.0",
    "yeoman-generator": "^4.11.0",
    "yosay": "^2.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.2",
    "@types/core-js": "^2.5.3",
    "@types/cross-spawn": "^6.0.2",
    "@types/eslint": "^7.2.0",
    "@types/inquirer": "^7.3.0",
    "@types/jest": "^26.0.8",
    "@types/lodash": "^4.14.158",
    "@types/prettier": "^2.0.2",
    "@types/yeoman-assert": "^3.1.1",
    "@types/yeoman-generator": "^4.11.0",
    "@types/yeoman-test": "^2.0.4",
    "@types/yosay": "^0.0.29",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "codecov": "^3.7.2",
    "conventional-changelog-cli": "^2.0.35",
    "conventional-changelog-conventionalcommits": "^4.3.1",
    "cpy-cli": "^3.1.1",
    "del-cli": "^3.0.1",
    "eslint": "7.6.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "jest": "^26.2.2",
    "jest-junit": "^11.1.0",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "prettier-plugin-organize-imports": "^1.1.1",
    "prettier-plugin-packagejson": "^2.2.5",
    "replace-in-files-cli": "^0.3.1",
    "ts-jest": "^26.1.4",
    "type-fest": "^0.16.0",
    "typescript": "^3.9.7",
    "watch": "^1.0.2",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^2.7.0"
  }
}
