{
  "name": "generator-startmeup",
  "version": "0.2.1",
  "description": "StartMeUp Yeoman Generator for starting a project with dotfiles and some common tools",
  "keywords": [],
  "homepage": "https://github.com/fernandopasik/generator-startmeup",
  "bugs": "https://github.com/fernandopasik/generator-startmeup/issues",
  "license": "MIT",
  "author": "Fernando Pasik <fernando@pasik.com.ar> (http://fernandopasik.com/)",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/fernandopasik/generator-startmeup.git"
  },
  "main": "generators/app/index.js",
  "scripts": {
    "start": "npm run watch",
    "clean": "rm -rf .tmp coverage",
    "lint": "jshint . --reporter=node_modules/jshint-stylish; jscs . --reporter=node_modules/jscs-stylish; eslint .",
    "test": "mocha --require mocha-clean",
    "pretest": "npm run clean",
    "cov": "istanbul cover _mocha --report lcovonly --report html -- -R spec --require mocha-clean",
    "precov": "npm run clean",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "precoveralls": "npm run -s cov",
    "travis": "npm run -s lint && npm run -s coveralls",
    "watch": "watch 'npm run -s lint; npm run -s cov' generators/ test/"
  },
  "config": {
    "blanket": {
      "pattern": "generators/"
    }
  },
  "files": [
    "generators"
  ],
  "dependencies": {
    "chalk": "^1.1.3",
    "underscore.string": "^3.3.4",
    "yeoman-generator": "^0.22.5",
    "yosay": "^1.1.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "^2.11.9",
    "escape-string-regexp": "^1.0.5",
    "eslint": "^2.7.0",
    "istanbul": "^0.4.3",
    "jscs": "^2.11.0",
    "jscs-stylish": "^0.3.1",
    "jshint": "^2.9.1",
    "jshint-stylish": "^2.1.0",
    "mocha": "^2.4.5",
    "mocha-clean": "^1.0.0",
    "sinon": "^1.17.3",
    "watch": "^0.17.1",
    "yeoman-assert": "^2.1.2",
    "yeoman-test": "^1.1.0"
  },
  "engines": {
    "node": ">=4.0.0",
    "npm": ">=1.2.10"
  }
}
