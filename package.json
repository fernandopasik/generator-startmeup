{
  "name": "generator-startmeup",
  "version": "0.25.1",
  "description": "StartMeUp Yeoman Generator for starting a project with dotfiles and some common tools",
  "keywords": [],
  "homepage": "https://github.com/fernandopasik/generator-startmeup",
  "bugs": "https://github.com/fernandopasik/generator-startmeup/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/fernandopasik/generator-startmeup.git"
  },
  "license": "MIT",
  "author": "Fernando Pasik <fernando@pasik.com.ar> (https://fernandopasik.com)",
  "contributors": [],
  "main": "generators/app/index.js",
  "bin": {
    "startmeup": "generators/startmeup.js"
  },
  "files": [
    "/generators"
  ],
  "scripts": {
    "prebuild": "del generators",
    "build": "tsc",
    "postbuild": "cpy **/templates/** ../generators/ --cwd=src --parents && chmod +x generators/startmeup.js",
    "prechangelog": "replace-in-files --string='hidden: true' --replacement='hidden: false' node_modules/conventional-changelog-conventionalcommits/writer-opts.js",
    "changelog": "conventional-changelog -p conventionalcommits -i CHANGELOG.md -s",
    "check-types": "tsc --noEmit -p tsconfig.all.json",
    "lint": "eslint . --ext ts,js",
    "start": "yarn build -w",
    "test": "jest",
    "verify": "yarn lint && yarn check-types && yarn test && yarn build",
    "preversion": "yarn verify"
  },
  "resolutions": {
    "github-username": "^5.0.1"
  },
  "dependencies": {
    "@jest/types": "^26.0.1",
    "core-js": "^3.6.5",
    "cross-spawn": "^7.0.3",
    "github-username": "^5.0.1",
    "inquirer": "^7.1.0",
    "lodash": "^4.17.15",
    "prettier": "^2.0.5",
    "sort-package-json": "^1.44.0",
    "yeoman-generator": "^4.10.1",
    "yosay": "^2.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/core-js": "^2.5.3",
    "@types/cross-spawn": "^6.0.2",
    "@types/eslint": "^6.8.1",
    "@types/inquirer": "^6.5.0",
    "@types/jest": "^25.2.3",
    "@types/lodash": "^4.14.155",
    "@types/prettier": "^2.0.1",
    "@types/yeoman-assert": "^3.1.1",
    "@types/yeoman-generator": "^3.1.4",
    "@types/yeoman-test": "^2.0.4",
    "@types/yosay": "^0.0.29",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "codecov": "^3.7.0",
    "conventional-changelog-cli": "^2.0.34",
    "conventional-changelog-conventionalcommits": "^4.3.0",
    "cpy-cli": "^3.1.1",
    "del-cli": "^3.0.1",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "jest-junit": "^10.0.0",
    "lint-staged": "^10.2.9",
    "prettier": "^2.0.5",
    "prettier-plugin-packagejson": "^2.2.5",
    "replace-in-files-cli": "^0.3.1",
    "ts-jest": "^26.1.0",
    "typescript": "^3.9.5",
    "watch": "^1.0.2",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^2.6.0"
  }
}
