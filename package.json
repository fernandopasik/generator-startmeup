{
  "name": "generator-startmeup",
  "version": "0.2.0",
  "description": "StartMeUp Yeoman Generator",
  "private": true,
  "homepage": "https://github.com/fernandopasik/generator-startmeup",
  "author": {
    "name": "Fernando Pasik",
    "email": "fernando@pasik.com.ar"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/fernandopasik/generator-startmeup.git"
  },
  "main": "generators/app/index.js",
  "scripts": {
    "start": "npm run watch",
    "clean": "rm -rf .tmp coverage",
    "lint": "jshint . --reporter=node_modules/jshint-stylish; jscs . --reporter=node_modules/jscs-stylish",
    "test": "mocha --require mocha-clean",
    "pretest": "npm run clean",
    "cov": "istanbul cover _mocha --report lcovonly --report html -- -R spec --require mocha-clean",
    "precov": "npm run clean",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "precoveralls": "npm run -s cov",
    "travis": "npm run -s lint && npm run -s coveralls",
    "watch": "watch 'npm run -s lint; npm run -s cov' generators/ test/"
  },
  "config": {
    "blanket": {
      "pattern": "generators/"
    }
  },
  "files": [
    "generators"
  ],
  "keywords": [],
  "dependencies": {
    "chalk": "^1.1.1",
    "underscore.string": "^3.2.3",
    "yeoman-generator": "^0.22.3",
    "yosay": "^1.1.0"
  },
  "devDependencies": {
    "chai": "^3.4.1",
    "coveralls": "^2.11.6",
    "escape-string-regexp": "^1.0.4",
    "istanbul": "^0.4.2",
    "jscs": "^2.8.0",
    "jscs-stylish": "^0.3.1",
    "jshint": "^2.9.1",
    "jshint-stylish": "^2.1.0",
    "mocha": "^2.3.4",
    "mocha-clean": "^1.0.0",
    "sinon": "^1.17.2",
    "watch": "^0.17.1",
    "yeoman-assert": "^2.1.1",
    "yeoman-test": "^1.0.0"
  },
  "engines": {
    "node": ">=0.10.0",
    "npm": ">=1.2.10"
  },
  "license": "MIT"
}
